{
	"mx-auto": {
		"style": {
			"marginLeft": "auto",
			"marginRight": "auto"
		}
	},
	"mx-8": {
		"style": {
			"marginLeft": 32,
			"marginRight": 32
		}
	},
	"mt-4": {
		"style": {
			"marginTop": 16
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"justify-between": {
		"style": {
			"justifyContent": "space-between"
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-gray-300": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(209 213 219 / var(--tw-border-opacity))"
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"py-2": {
		"style": {
			"paddingTop": 8,
			"paddingBottom": 8
		}
	},
	"py-1": {
		"style": {
			"paddingTop": 4,
			"paddingBottom": 4
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-3xl": {
		"style": {
			"fontSize": 30,
			"lineHeight": 36
		}
	},
	"text-sm": {
		"style": {
			"fontSize": 14,
			"lineHeight": 20
		}
	},
	"text-base": {
		"style": {
			"fontSize": 16,
			"lineHeight": 24
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"text-black": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(0 0 0 / var(--tw-text-opacity))"
		}
	},
	"text-primary": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(53 168 148 / var(--tw-text-opacity))"
		}
	},
	"text-rose-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(159 18 57 / var(--tw-text-opacity))"
		}
	}
}